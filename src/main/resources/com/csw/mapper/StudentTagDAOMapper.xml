<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.csw.dao.StudentTagDAO">

    <insert id="save" parameterType="studentTag">
        insert into t_student_tag values (#{sid},#{tid})
    </insert>

    <delete id="delete" parameterType="string">
        delete from t_student_tag where sid = #{sid}
    </delete>


</mapper>
